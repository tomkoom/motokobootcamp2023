{
  "canisters": {
    "dao": {
      "main": "canisters/dao/main.mo",
      "type": "motoko"
    },
    "webpage": {
      "main": "canisters/webpage/main.mo",
      "type": "motoko"
    },
    "assets": {
      "dependencies": ["dao", "webpage"],
      "frontend": {
        "entrypoint": "dist/index.html"
      },
      "source": ["dist/"],
      "type": "assets"
    }
  },
  "defaults": {
    "build": {
      "packtool": ""
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    },
    "ic": {
      "providers": ["https://mainnet.dfinity.network"],
      "type": "persistent"
    }
  },
  "version": 1
}
